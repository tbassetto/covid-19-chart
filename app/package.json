{
  "name": "app",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Thomas Bassetto <thobasse@cisco.com>",
  "license": "MIT",
  "scripts": {
    "build": "concurrently -n \"server,app\" -c \"bgBlue.bold,bgMagenta.bold\" -s \"first\" -k \"yarn server\" \"next build\"",
    "dev": "concurrently -n \"server,app\" -c \"bgBlue.bold,bgMagenta.bold\" -s \"first\" -k \"yarn server:dev\" \"next\"",
    "export": "next export",
    "lint": "eslint *.ts components/*.tsx pages/*.tsx",
    "prettier": "prettier *.ts components pages --write",
    "server": "yarn --cwd ../api build && yarn --cwd ../api start",
    "server:dev": "yarn --cwd ../api dev",
    "start": "next start"
  },
  "dependencies": {
    "@chakra-ui/core": "^0.8.0",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@vx/axis": "^0.0.198",
    "@vx/curve": "^0.0.198",
    "@vx/event": "^0.0.198",
    "@vx/gradient": "^0.0.198",
    "@vx/grid": "^0.0.198",
    "@vx/group": "^0.0.198",
    "@vx/mock-data": "^0.0.198",
    "@vx/responsive": "^0.0.198",
    "@vx/scale": "^0.0.198",
    "@vx/shape": "^0.0.198",
    "@vx/text": "^0.0.198",
    "@vx/tooltip": "^0.0.198",
    "api": "^1.0.0",
    "color-hash-ts": "^0.0.7",
    "d3-array": "^2.4.0",
    "d3-format": "^1.4.3",
    "d3-time-format": "^2.2.3",
    "downshift": "^5.0.5",
    "emotion-theming": "^10.0.27",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.3.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/node": "^12.9.0",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "v3.6.0",
    "@typescript-eslint/parser": "v3.6.0",
    "concurrently": "^5.1.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "prettier": "^2.0.1",
    "typescript": "^3.8.3"
  }
}
